background: none;
width: 15px;
subcontrol-position: right;
subcontrol-origin: margin;
}
QScrollBar::left-arrow:horizontal {
background-image: url(:/styles/scrollbar_h_leftArrow.png);
background-repeat: no repeat;
width: 15px;
height: 15px;
}
QScrollBar::left-arrow:horizontal:hover {
background-image: url(:/styles/scrollbar_h_leftArrow_hover.png);
background-repeat: no repeat;
width: 15px;
height: 15px;
}
QScrollBar::left-arrow:horizontal:pressed {
background-image: url(:/styles/scrollbar_h_leftArrow_pressed.png);
background-repeat: no repeat;
width: 15px;
height: 15px;
}
QScrollBar::right-arrow:horizontal {
background-image: url(:/styles/scrollbar_h_rightArrow.png);
background-repeat: no repeat;
width: 15px;
height: 15px;
}
QScrollBar::right-arrow:horizontal:hover {
background-image: url(:/styles/scrollbar_h_rightArrow_hover.png);
background-repeat: no repeat;
width: 15px;
height: 15px;
}
QScrollBar::right-arrow:horizontal:pressed {
background-image: url(:/styles/scrollbar_h_rightArrow_pressed.png);
background-repeat: no repeat;
