}

QLineEdit, QSpinBox, QDoubleSpinBox, QTimeEdit {
max-height: 17px;
border-image: url(:/styles/lineEditBg.png) 3 3 3 3 stretch;
border-width: 3px 3px 3px 3px;
border-radius: 0px;
}
QLineEdit:hover, QSpinBox:hover, QDoubleSpinBox:hover, QTimeEdit:hover {
border-image: url(:/styles/lineEditBg_hover.png) 3 3 3 3 stretch;
border-width: 3px 3px 3px 3px;
border-radius: 0px;
}
QLineEdit:focus, QSpinBox:focus, QDoubleSpinBox:focus, QTimeEdit:focus {
border-image: url(:/styles/lineEditBg_pressed.png) 3 3 3 3 stretch;
border-width: 3px 3px 3px 3px;
border-radius: 0px;
}
QLineEdit:disabled, QSpinBox:disabled, QDoubleSpinBox:disabled, QTimeEdit:disabled {
border-image: url(:/styles/lineEditBg_disabled.png) 3 3 3 3 stretch;
border-width: 3px 3px 3px 3px;
border-radius: 0px;
}

QScrollArea, QTableView, QTextBrowser, QTextEdit {
background-color: #f0f0f0;
border-color: #969696;
border-style: solid;
border-radius: 3px;
border-width: 2px 2px 2px 2px;

/*border-image: url(:/styles/wsChatDisplayBg.png) 3 3 3 3 stretch;
border-width: 1px 1px 1px 1px;*/
}

QScrollArea:focus, QTableView:focus, QTextBrowser:focus, QListWidget:focus, QTextEdit:focus, QTreeWidget:focus {
